            POST /cgi-bin/statusprocess.exe HTTP/1.1
            Host: %%%routerip%%%
            Cookie: %%%Set-Cookie%%%

disconnect=disconnect
        [[[/REQUEST]]]
    [[[/STEP]]]
    [[[STEP]]][[[WAIT seconds="3"/]]][[[/STEP]]]
    [[[STEP]]]
        [[[REQUEST]]]
            POST /cgi-bin/statusprocess.exe HTTP/1.1
            Host: %%%routerip%%%
            Cookie: %%%Set-Cookie%%%

connect=connect
        [[[/REQUEST]]]
    [[[/STEP]]]
[[[/HSRC]]]